{{!--
  This partial renders the API overview for the documentation.
  @param {object} this - The documentation context, including all assemblies, namespaces, types, and topics.
  @note The common template variables are not available in this context.
--}}
{{#if (eq (len assemblies) 1)}}
{{>assembly level=1 assembly=assemblies.0}}
{{else}}
<h1>Assemblies</h1>
{{#each assemblies}}
<div class="assembly-section">
  {{>assembly level=2 assembly=this}}
</div>
{{/each}}
{{/if}}